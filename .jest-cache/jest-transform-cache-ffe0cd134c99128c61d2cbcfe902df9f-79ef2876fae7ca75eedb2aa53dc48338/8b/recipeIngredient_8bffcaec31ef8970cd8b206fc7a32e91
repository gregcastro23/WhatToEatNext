/**
 * Validates that an object conforms to the RecipeIngredient interface
 */
function validateIngredient(obj) {
    return obj &&
        typeof obj.name === 'string' &&
        typeof obj.amount === 'number' &&
        typeof obj.unit === 'string';
}
