{
  "name": "what-to-eat-next",
  "version": "1.0.0",
  "description": "A personalized food recommendation system based on astrological data and elemental balance",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:unused": "eslint . --ext .js,.jsx,.ts,.tsx --fix --rule '@typescript-eslint/no-unused-vars: [\"warn\", { \"argsIgnorePattern\": \"^_\", \"varsIgnorePattern\": \"^_\", \"caughtErrorsIgnorePattern\": \"^_\" }]'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.0",
    "astronomia": "^4.1.1",
    "astronomy-engine": "^2.1.19",
    "axios": "^1.8.3",
    "framer-motion": "^12.5.0",
    "lucide-react": "^0.482.0",
    "next": "14.1.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@humanwhocodes/module-importer": "^1.0.1",
    "@testing-library/jest-dom": "^5.0.0",
    "@testing-library/react": "^13.0.0",
    "@types/axios": "^0.14.4",
    "@types/jest": "^29.0.0",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^9.22.0",
    "eslint-import-resolver-typescript": "^3.9.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "jest": "^29.0.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1",
    "typescript-eslint": "^8.26.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "type": "commonjs"
}
