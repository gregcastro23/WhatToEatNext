  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=4.0.0"
  },
  "resolutions": {
    "@next/swc-darwin-x64": "15.3.4"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf .next dist build out .turbo",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,astro}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,astro}\"",
    "prepare": "husky install",
    "postinstall": "husky install",
    "lint:comprehensive": "node scripts/lint-fixes/comprehensive-lint-fix.js",
