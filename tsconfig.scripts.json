{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "noEmit": false,
    "outDir": "dist-scripts",
    "module": "commonjs",
    "target": "ES2022",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "declaration": false,
    "sourceMap": true,
    "skipLibCheck": true
  },
  "include": [
    "src/scripts/unused-vars/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
