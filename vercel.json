{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "corepack yarn build",
  "installCommand": "corepack yarn install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 10
    }
  },
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1"
  }
}
