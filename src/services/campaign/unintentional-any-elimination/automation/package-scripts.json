{
  "scripts": {
    "any:analyze": "node unintentional-any-eliminator.cjs --max-files=50 --dry-run --target=0",
    "any:classify": "node src/services/campaign/unintentional-any-elimination/cli/unintentional-any-cli.cjs classify --confidence=0.8",
    "any:fix": "node unintentional-any-eliminator.cjs --max-files=25 --safety=MAXIMUM --target=15",
    "any:fix-conservative": "node fix-explicit-any-enhanced.cjs --max-files=10 --safety=MAXIMUM",
    "any:fix-aggressive": "node unintentional-any-eliminator.cjs --max-files=50 --safety=HIGH --target=25",
    "any:batch": "node src/services/campaign/unintentional-any-elimination/cli/batch-processor.cjs process --max-files=15",
    "any:monitor": "node src/services/campaign/unintentional-any-elimination/cli/progress-monitor.cjs start --interval=30",
    "any:debug": "node src/services/campaign/unintentional-any-elimination/cli/debug-tools.cjs system",
    "any:debug-component": "node src/services/campaign/unintentional-any-elimination/cli/debug-tools.cjs component --name=AnyTypeClassifier",
    "any:report": "node src/services/campaign/unintentional-any-elimination/cli/unintentional-any-cli.cjs report --format=html",
    "any:validate": "node src/services/campaign/unintentional-any-elimination/cli/unintentional-any-cli.cjs validate --build --safety",
    "any:scheduler": "node src/services/campaign/unintentional-any-elimination/automation/campaign-scheduler.cjs start",
    "any:scheduler-status": "node src/services/campaign/unintentional-any-elimination/automation/campaign-scheduler.cjs status",
    "any:emergency": "node src/services/campaign/unintentional-any-elimination/automation/campaign-scheduler.cjs trigger emergency-response",
    "any:cli": "node src/services/campaign/unintentional-any-elimination/cli/index.js",
    "any:help": "node src/services/campaign/unintentional-any-elimination/cli/index.js help"
  },
  "description": "Package.json script integration for Unintentional Any Elimination system",
  "integration": {
    "existing_scripts": {
      "lint": "Enhanced to work with any elimination system",
      "lint:fix": "Can be combined with any:fix for comprehensive cleanup",
      "test": "Should be run after any elimination to ensure no regressions",
      "build": "Automatically validated by any elimination safety protocols"
    },
    "workflow_integration": {
      "pre-commit": [
        "yarn any:validate",
        "yarn lint",
        "yarn test"
      ],
      "daily": [
        "yarn any:analyze",
        "yarn any:monitor --interval=3600"
      ],
      "weekly": [
        "yarn any:fix",
        "yarn any:report"
      ],
      "emergency": [
        "yarn any:emergency"
      ]
    },
    "ci_cd_integration": {
      "quality_gates": {
        "typescript_errors": "yarn tsc --noEmit --skipLibCheck",
        "explicit_any_count": "yarn lint --format=unix | grep -c '@typescript-eslint/no-explicit-any' || true",
        "build_validation": "yarn build"
      },
      "automated_fixes": {
        "trigger_threshold": 100,
        "safety_level": "MAXIMUM",
        "max_files_per_run": 10
      }
    }
  },
  "environment_variables": {
    "MAX_FILES": "Maximum files to process per batch (default: 25)",
    "SAFETY_LEVEL": "Safety level: MAXIMUM|HIGH|MEDIUM (default: MAXIMUM)",
    "BATCH_SIZE": "Batch processing size (default: 15)",
    "CONFIDENCE_THRESHOLD": "Classification confidence threshold 0-1 (default: 0.8)",
    "TARGET_REDUCTION": "Target reduction percentage (default: 15.0)",
    "DRY_RUN": "Enable dry run mode (default: false)",
    "CONTINUE_ON_ERROR": "Continue processing on errors (default: false)",
    "ENABLE_CAMPAIGN": "Enable campaign integration (default: true)",
    "LOG_LEVEL": "Logging level: error|warn|info|debug (default: info)",
    "OUTPUT_DIR": "Output directory for reports (default: ./reports)",
    "BACKUP_DIR": "Backup directory (default: ./backups)",
    "ALERT_THRESHOLD": "Alert threshold for monitoring (default: 100)",
    "MONITORING_INTERVAL": "Monitoring interval in seconds (default: 30)"
  },
  "examples": {
    "basic_analysis": {
      "command": "yarn any:analyze",
      "description": "Run comprehensive analysis without making changes"
    },
    "conservative_fix": {
      "command": "yarn any:fix-conservative",
      "description": "Apply only the safest fixes with maximum safety protocols"
    },
    "targeted_fix": {
      "command": "MAX_FILES=10 SAFETY_LEVEL=MAXIMUM yarn any:fix",
      "description": "Fix up to 10 files with maximum safety"
    },
    "monitoring": {
      "command": "yarn any:monitor",
      "description": "Start real-time monitoring of progress"
    },
    "batch_processing": {
      "command": "BATCH_SIZE=5 yarn any:batch",
      "description": "Process files in small batches of 5"
    },
    "debug_system": {
      "command": "yarn any:debug",
      "description": "Run system diagnostics"
    },
    "generate_report": {
      "command": "yarn any:report",
      "description": "Generate HTML progress report"
    },
    "validate_system": {
      "command": "yarn any:validate",
      "description": "Validate build and safety protocols"
    },
    "emergency_response": {
      "command": "yarn any:emergency",
      "description": "Trigger emergency response campaign"
    },
    "scheduler_daemon": {
      "command": "yarn any:scheduler",
      "description": "Start automated campaign scheduler"
    }
  },
  "troubleshooting": {
    "build_failures": {
      "description": "If TypeScript build fails after fixes",
      "solution": "Safety protocols should automatically rollback. Check logs and run yarn any:validate"
    },
    "high_error_count": {
      "description": "If error count increases instead of decreases",
      "solution": "Run yarn any:debug to diagnose issues. Consider lowering confidence threshold"
    },
    "performance_issues": {
      "description": "If processing is too slow",
      "solution": "Reduce MAX_FILES and BATCH_SIZE. Check system resources with yarn any:debug"
    },
    "classification_errors": {
      "description": "If classification is inaccurate",
      "solution": "Adjust CONFIDENCE_THRESHOLD. Run yarn any:debug-component to test classifier"
    },
    "campaign_failures": {
      "description": "If automated campaigns fail",
      "solution": "Check yarn any:scheduler-status and review logs. Ensure safety protocols are working"
    }
  }
}
